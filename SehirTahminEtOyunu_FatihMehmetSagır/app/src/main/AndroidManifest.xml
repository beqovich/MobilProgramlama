<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ornekdenem.mobluygulma">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Sehir Tahmin Et Oyunu"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Mobluygulma">
        <activity
            android:name="com.ornekdenem.mobluygulma.HakkindaActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ornekdenem.mobluygulma.NormalOyunAktivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.ornekdenem.mobluygulma.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>